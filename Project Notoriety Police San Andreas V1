         01AF:   car trains4 sphere 0 in_sphere -1943.1394 192.879 25.6744 radius 7.2 7.2 4.0
         01AF:   car trains4 sphere 0 in_sphere  1487.4427 2634.2415 11.0626 radius 7.2 7.2 4.0
         01AF:   car trains4 sphere 0 in_sphere  2865.0713 1214.1335 11.2212 radius 7.2 7.2 4.0
         01AF:   car trains4 sphere 0 in_sphere  1671.9703 -1955.802 14.0739    radius 7.2 7.2  4.0
         01AF:   car trains4 sphere 0 in_sphere  773.5843 -1332.6434 -1.5124 radius 7.2 7.2  4.0

 
 
      then
     TRAIN_SET_TO_STOP_SA2 = true
         TIMER_CUSTOM2_SA = 0 
 
         TRAIN_STOPS_SA = 1
         06DC: set_train trains4 acc 0.0
        06DD: set_train trains4 speed 0.0
         
     end 
 end 
 
 
    if    and                 //train stops player enters
    TIMER_CUSTOM2_SA  > 13000
    TRAIN_SET_TO_STOP_SA2 == true
    then
    TRAIN_SET_TO_STOP_SA2 = false 
    TIMER_CUSTOM2_SA = 0 
          06DC: set_train trains4 acc 80.0
        06DD: set_train trains4 speed 80.0
    end 
          
     
