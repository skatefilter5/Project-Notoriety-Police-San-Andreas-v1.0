actor.GiveWeaponAndAmmo(xENEMY6x, WEAPONS_STYLE, 99999)
0446: set_actor xENEMY6x dismemberment_possible HEADSHOT_MODE
07FE: set_actor xENEMY6x fighting_style_to FIGHTING_STYLE moves 6
07DD: set_actor xENEMY6x attack_rate FIRE_RATE // previously known as temper_to
0946: set_actor xENEMY6x actions_uninterupted_by_weapon_fire FLINCH
///6x kill_actor $player_actor
//081A: set_actor xENEMY6x weapon_skill_to SKILL_LEVEL
//060F: set_actor xENEMY6x melee_accuracy_to MELEE_LEVEL
05DE: set_actor xENEMY6x walk_paths
03FE: set_actor xENEMY6x cash_give MONEY_LEVEL
04D8: set_actor xENEMY6x drowns_in_water WATER_DROWNS
09B5: set_actor xENEMY6x signal_after_kill 0
// set_actor xENEMY6x supporting_fire 0
060B: set_actor xENEMY6x decision_maker_to 780@     // BP 0 FP 0 EP 0 CP 0 MP 0
077A: set_actor xENEMY6x acquaintance PLAYER_HATE to_actors_pedtype 0 // see ped.dat
077A: set_actor xENEMY6x acquaintance PLAYER_HATE to_actors_pedtype 1 // see ped.dat
                      
xMARKER6x = marker.CreateAboveActor(xENEMY6x)
marker.SetColor(xMARKER6x,0x454599FF)
marker.SetIconSize(xMARKER6x, 1)
018B: set_marker xMARKER6x radar_mode MARKER_RADAR_MODE

    if
    056D:   actor Police_goto_target defined
    then
           05E2: AS_actor xENEMY6x kill_actor Police_goto_target

    end
        if
    $notoriety >= 1
    then
    05E2: AS_actor xENEMY6x kill_actor $player_actor
    end 

