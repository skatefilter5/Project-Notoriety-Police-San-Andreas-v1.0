         04A9:   actor $PLAYER_ACTOR driving_heli
         actor.DrivingPlane($PLAYER_ACTOR)
         04A7:   actor $PLAYER_ACTOR driving_boat

         then
         LOSE_THE_COP_TIME -= 7.0

         end          
 end
  if  and                      
 wave >= 4
 wave < 7
 then
 LOSE_THE_COP_TIME -= 1.0
          if
         0597:   actor $PLAYER_ACTOR crouching
         then
         LOSE_THE_COP_TIME -= 2.0
         end 
         if
           0965:   actor $PLAYER_ACTOR swimming
        
         then
         LOSE_THE_COP_TIME -= 3.5
         end 
         if  or
         04A9:   actor $PLAYER_ACTOR driving_heli
         actor.DrivingPlane($PLAYER_ACTOR)
         04A7:   actor $PLAYER_ACTOR driving_boat

         then
         LOSE_THE_COP_TIME -= 6.0

         end 
 end
 if
 wave >= 7
 then
 LOSE_THE_COP_TIME -= 0.8
 
         if
         0597:   actor $PLAYER_ACTOR crouching
         then
         LOSE_THE_COP_TIME -= 1.0
         end 
         if
           0965:   actor $PLAYER_ACTOR swimming
        
         then
         LOSE_THE_COP_TIME -= 3.0
         end 
          if  or
         04A9:   actor $PLAYER_ACTOR driving_heli
         actor.DrivingPlane($PLAYER_ACTOR)
         04A7:   actor $PLAYER_ACTOR driving_boat

         then
         LOSE_THE_COP_TIME -= 5.0

         end         
 end
 
//SPAWN_LEVEL += 100
end
 
 
  
return 
 

 



  

