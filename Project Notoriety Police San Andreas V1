    car.SetToPsychoDriver(RIDE6_CAR)
   car.Angle(RIDE6_CAR)=345@
    00AD: normal__car RIDE6_CAR max_speed_to 15.0
    //0229: set_car RIDE6_CAR primary_color_to GANG_CAR_COLOR1 secondary_color_to GANG_CAR_COLOR2
       //
    gosub @WAVEs 
    xPOLICE_DRIVER_CAR6_ = actor.CreateAsDriver(TEAM_A, RANDOM_ENEMY_MODEL,RIDE6_CAR)
 
  
    actor.Health(xPOLICE_DRIVER_CAR6_) =  HEALTH_LEVEL
    actor.WeaponAccuracy(xPOLICE_DRIVER_CAR6_) = ACCURACY_LEVEL
    actor.GiveWeaponAndAmmo(xPOLICE_DRIVER_CAR6_, WEAPONS_STYLE, 99999)
    0446: set_actor xPOLICE_DRIVER_CAR6_ dismemberment_possible HEADSHOT_MODE
    07FE: set_actor xPOLICE_DRIVER_CAR6_ fighting_style_to FIGHTING_STYLE moves 6
    07DD: set_actor xPOLICE_DRIVER_CAR6_ attack_rate FIRE_RATE // previously known as temper_to
    0946: set_actor xPOLICE_DRIVER_CAR6_ actions_uninterupted_by_weapon_fire FLINCH
    //081A: set_actor xPOLICE_DRIVER_CAR6_ weapon_skill_to SKILL_LEVEL
    //060F: set_actor xPOLICE_DRIVER_CAR6_ melee_accuracy_to MELEE_LEVEL
    03FE: set_actor xPOLICE_DRIVER_CAR6_ cash_give MONEY_LEVEL
    04D8: set_actor xPOLICE_DRIVER_CAR6_ drowns_in_water WATER_DROWNS
    09B5: set_actor xPOLICE_DRIVER_CAR6_ signal_after_kill 0
    // set_actor xPOLICE_DRIVER_CAR6_ supporting_fire 0
    060B: set_actor xPOLICE_DRIVER_CAR6_ decision_maker_to 780@     // BP 0 FP 0 EP 0 CP 0 MP 0
    077A: set_actor xPOLICE_DRIVER_CAR6_ acquaintance PLAYER_HATE to_actors_pedtype 0 // see ped.dat
    077A: set_actor xPOLICE_DRIVER_CAR6_ acquaintance PLAYER_HATE to_actors_pedtype 1 // see ped.dat
    
       gosub @WAVEs 
    xCOP_PASSENGER1_CAR6_ = actor.CreateAsPassenger(TEAM_A, RANDOM_ENEMY_MODEL,RIDE6_CAR,0)
 
  
    actor.Health(xCOP_PASSENGER1_CAR6_) =  HEALTH_LEVEL
    actor.WeaponAccuracy(xCOP_PASSENGER1_CAR6_) = ACCURACY_LEVEL
    actor.GiveWeaponAndAmmo(xCOP_PASSENGER1_CAR6_, WEAPONS_STYLE, 99999)
    0446: set_actor xCOP_PASSENGER1_CAR6_ dismemberment_possible HEADSHOT_MODE
    07FE: set_actor xCOP_PASSENGER1_CAR6_ fighting_style_to FIGHTING_STYLE moves 6
    07DD: set_actor xCOP_PASSENGER1_CAR6_ attack_rate FIRE_RATE // previously known as temper_to
    0946: set_actor xCOP_PASSENGER1_CAR6_ actions_uninterupted_by_weapon_fire FLINCH
    //081A: set_actor xCOP_PASSENGER1_CAR6_ weapon_skill_to SKILL_LEVEL
    //060F: set_actor xCOP_PASSENGER1_CAR6_ melee_accuracy_to MELEE_LEVEL
    03FE: set_actor xCOP_PASSENGER1_CAR6_ cash_give MONEY_LEVEL
    04D8: set_actor xCOP_PASSENGER1_CAR6_ drowns_in_water WATER_DROWNS
    09B5: set_actor xCOP_PASSENGER1_CAR6_ signal_after_kill 0
    // set_actor xCOP_PASSENGER1_CAR6_ supporting_fire 0
    060B: set_actor xCOP_PASSENGER1_CAR6_ decision_maker_to 780@     // BP 0 FP 0 EP 0 CP 0 MP 0
    077A: set_actor xCOP_PASSENGER1_CAR6_ acquaintance PLAYER_HATE to_actors_pedtype 0 // see ped.dat
    077A: set_actor xCOP_PASSENGER1_CAR6_ acquaintance PLAYER_HATE to_actors_pedtype 1 // see ped.dat
    
 
     
 
                          
    Xmarker37x = marker.CreateAboveActor(xCOP_PASSENGER1_CAR6_)
    marker.SetColor(Xmarker37x,0x454599FF)
    marker.SetIconSize(Xmarker37x, 1)
    018B: set_marker Xmarker37x radar_mode MARKER_RADAR_MODE
  
                          
    Xmarker36x = marker.CreateAboveActor(xPOLICE_DRIVER_CAR6_)
    marker.SetColor(Xmarker36x,0x454599FF)
    marker.SetIconSize(Xmarker36x, 1)
    018B: set_marker Xmarker36x radar_mode MARKER_RADAR_MODE
 
    
   0840: link_car RIDE6_CAR to_interior 983@ 
 
         if  and
        $notoriety >= 1
        $notoriety < 9
        then
           0397: enable_car RIDE6_CAR siren 1
       00AE: set_car RIDE6_CAR traffic_behaviour_to 2
        00AD: hostile_car RIDE6_CAR max_speed_to 120.0
        car.SetDriverBehaviour(RIDE6_CAR, DriveToPlayer)  
        end 
         if
        $notoriety >= 9
        then
         0397: enable_car RIDE6_CAR siren 1
        00AE: set_car RIDE6_CAR traffic_behaviour_to 2
        00AD: hostile_v2_car RIDE6_CAR max_speed_to 190.0
        car.SetDriverBehaviour(RIDE6_CAR, KillThePlayer) 
        end 
                
        if
        $notoriety >= 11
       
        then
          03AB: set_car RIDE6_CAR strong 1
        end 
          CAR.RemoveReferences(RIDE6_CAR)   
end


