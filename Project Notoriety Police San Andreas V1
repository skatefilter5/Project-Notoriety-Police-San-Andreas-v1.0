  856D:  not car RIDE3_CAR defined
001D: Timer_custom8_Gang_Car > cop_spawn_level
wave >= 1
then
Timer_custom8_Gang_Car = 0 
        
    RIDE3_CAR = car.Create(GANG_CAR_MODEL, 883@, 884@, 885@)
    car.Health(RIDE3_CAR) = 6000
    car.SetToPsychoDriver(RIDE3_CAR)
   car.Angle(RIDE3_CAR)=345@
    00AD: normal__car RIDE3_CAR max_speed_to 15.0
    //0229: set_car RIDE3_CAR primary_color_to GANG_CAR_COLOR1 secondary_color_to GANG_CAR_COLOR2
       //
