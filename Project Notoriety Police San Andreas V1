                                      car.SetMaxSpeed(cop_car_for_NPCS, 50.0)
                                      car.RemoveReferences(cop_car_for_NPCS)
                                      
                                                                                                                  
                                                if and
                                                056D:   actor xPOLICE_DRIVER_CAR6_ defined
                                                056D:   actor xCOP_PASSENGER1_CAR6_ defined 
                                                then
                          //  actor xCOP_PASSENGER1_CAR6_ driveby_actor RANDOM_LOSER_ASS car -1 point 0.0 0.0 0.0 radius 500.0 4 1 firing_rate 90
                                    
                                                
