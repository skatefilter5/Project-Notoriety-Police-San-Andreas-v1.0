         0713: actor ARMY_10 driveby_actor $PLAYER_ACTOR car -1 point 0.0 0.0 0.0 radius 700.0 4 1 firing_rate 100
        0713: actor ARMY_11 driveby_actor $PLAYER_ACTOR car -1 point 0.0 0.0 0.0 radius 700.0 4 1 firing_rate 100
        0713: actor ARMY_12 driveby_actor $PLAYER_ACTOR car -1 point 0.0 0.0 0.0 radius 700.0 4 1 firing_rate 100
       end     
         if  and
        $notoriety >= 1
        $notoriety < 9
        then
                0397: enable_car Troops_car_3 siren 1

       00AE: set_car Troops_car_3 traffic_behaviour_to 2
        00AD: hostile_car Troops_car_3 max_speed_to 120.0
        car.SetDriverBehaviour(Troops_car_3, DriveToPlayer)  
        end 
         if
        $notoriety >= 9
        then
            0397: enable_car Troops_car_3 siren 1
        00AE: set_car Troops_car_3 traffic_behaviour_to 2
        00AD: hostile_v2_car Troops_car_3 max_speed_to 190.0
        car.SetDriverBehaviour(Troops_car_3, KillThePlayer) 
        end 
                
        if
        $notoriety >= 11
       
        then
          03AB: set_car Troops_car_3 strong 1
        end 
        CAR.RemoveReferences(Troops_car_3)   
end 
return 

