    060B: set_actor xCOP_PASSENGER1_CAR2_ decision_maker_to 780@     // BP 0 FP 0 EP 0 CP 0 MP 0
    077A: set_actor xCOP_PASSENGER1_CAR2_ acquaintance PLAYER_HATE to_actors_pedtype 0 // see ped.dat
    077A: set_actor xCOP_PASSENGER1_CAR2_ acquaintance PLAYER_HATE to_actors_pedtype 1 // see ped.dat
     
    actor.Health(xCOP_PASSENGER2_CAR2_) =  HEALTH_LEVEL
    actor.WeaponAccuracy(xCOP_PASSENGER2_CAR2_) = ACCURACY_LEVEL
    actor.GiveWeaponAndAmmo(xCOP_PASSENGER2_CAR2_, WEAPONS_STYLE, 99999)
    0446: set_actor xCOP_PASSENGER2_CAR2_ dismemberment_possible HEADSHOT_MODE
    07FE: set_actor xCOP_PASSENGER2_CAR2_ fighting_style_to FIGHTING_STYLE moves 6
    07DD: set_actor xCOP_PASSENGER2_CAR2_ attack_rate FIRE_RATE // previously known as temper_to
    0946: set_actor xCOP_PASSENGER2_CAR2_ actions_uninterupted_by_weapon_fire FLINCH
    //081A: set_actor xCOP_PASSENGER2_CAR2_ weapon_skill_to SKILL_LEVEL
    //060F: set_actor xCOP_PASSENGER2_CAR2_ melee_accuracy_to MELEE_LEVEL
    03FE: set_actor xCOP_PASSENGER2_CAR2_ cash_give MONEY_LEVEL
    04D8: set_actor xCOP_PASSENGER2_CAR2_ drowns_in_water WATER_DROWNS
    09B5: set_actor xCOP_PASSENGER2_CAR2_ signal_after_kill 0
    // set_actor xCOP_PASSENGER2_CAR2_ supporting_fire 0
    060B: set_actor xCOP_PASSENGER2_CAR2_ decision_maker_to 780@     // BP 0 FP 0 EP 0 CP 0 MP 0
    077A: set_actor xCOP_PASSENGER2_CAR2_ acquaintance PLAYER_HATE to_actors_pedtype 0 // see ped.dat
    077A: set_actor xCOP_PASSENGER2_CAR2_ acquaintance PLAYER_HATE to_actors_pedtype 1 // see ped.dat
     
    actor.Health(xCOP_PASSENGER3_CAR2_) =  HEALTH_LEVEL
    actor.WeaponAccuracy(xCOP_PASSENGER3_CAR2_) = ACCURACY_LEVEL
    actor.GiveWeaponAndAmmo(xCOP_PASSENGER3_CAR2_, WEAPONS_STYLE, 99999)
    0446: set_actor xCOP_PASSENGER3_CAR2_ dismemberment_possible HEADSHOT_MODE
    07FE: set_actor xCOP_PASSENGER3_CAR2_ fighting_style_to FIGHTING_STYLE moves 6
    07DD: set_actor xCOP_PASSENGER3_CAR2_ attack_rate FIRE_RATE // previously known as temper_to
    0946: set_actor xCOP_PASSENGER3_CAR2_ actions_uninterupted_by_weapon_fire FLINCH
    //081A: set_actor xCOP_PASSENGER3_CAR2_ weapon_skill_to SKILL_LEVEL
    //060F: set_actor xCOP_PASSENGER3_CAR2_ melee_accuracy_to MELEE_LEVEL
    03FE: set_actor xCOP_PASSENGER3_CAR2_ cash_give MONEY_LEVEL
    04D8: set_actor xCOP_PASSENGER3_CAR2_ drowns_in_water WATER_DROWNS
    09B5: set_actor xCOP_PASSENGER3_CAR2_ signal_after_kill 0
    // set_actor xCOP_PASSENGER3_CAR2_ supporting_fire 0
    060B: set_actor xCOP_PASSENGER3_CAR2_ decision_maker_to 780@     // BP 0 FP 0 EP 0 CP 0 MP 0
    077A: set_actor xCOP_PASSENGER3_CAR2_ acquaintance PLAYER_HATE to_actors_pedtype 0 // see ped.dat
    077A: set_actor xCOP_PASSENGER3_CAR2_ acquaintance PLAYER_HATE to_actors_pedtype 1 // see ped.dat
     
                          
    Xmarker25X = marker.CreateAboveActor(xPOLICE_DRIVER_CAR2_)
    marker.SetColor(Xmarker25x,0x454599FF)
    marker.SetIconSize(Xmarker25X, 1)
    018B: set_marker Xmarker25X radar_mode MARKER_RADAR_MODE
    
    Xmarker26X = marker.CreateAboveActor(xCOP_PASSENGER1_CAR2_)
    marker.SetColor(Xmarker26x,0x454599FF)
    marker.SetIconSize(Xmarker26X, 1)
    018B: set_marker Xmarker26X radar_mode MARKER_RADAR_MODE
    
    Xmarker27X = marker.CreateAboveActor(xCOP_PASSENGER2_CAR2_)
    marker.SetColor(Xmarker27x,0x454599FF)
    marker.SetIconSize(Xmarker27X, 1)
    018B: set_marker Xmarker27X radar_mode MARKER_RADAR_MODE
    
    Xmarker28X = marker.CreateAboveActor(xCOP_PASSENGER3_CAR2_)
    marker.SetColor(Xmarker28x,0x454599FF)
    marker.SetIconSize(Xmarker28X, 1)
    018B: set_marker Xmarker28X radar_mode MARKER_RADAR_MODE
   0840: link_car RIDE2_CAR to_interior 983@ 
    
         if  and
        $notoriety >= 1
        $notoriety < 9
        then
                0397: enable_car RIDE2_CAR siren 1

       00AE: set_car RIDE2_CAR traffic_behaviour_to 2
        00AD: hostile_car RIDE2_CAR max_speed_to 120.0
        car.SetDriverBehaviour(RIDE2_CAR, DriveToPlayer)  
        end 
         if
        $notoriety >= 9
        then
            0397: enable_car RIDE2_CAR siren 1
        00AE: set_car RIDE2_CAR traffic_behaviour_to 2
        00AD: hostile_v2_car RIDE2_CAR max_speed_to 190.0
        car.SetDriverBehaviour(RIDE2_CAR, KillThePlayer) 
        end 
                
        if
        $notoriety >= 11
       
        then
          03AB: set_car RIDE2_CAR strong 1
        end 
          CAR.RemoveReferences(RIDE2_CAR)   
end

if and
856D: not  actor xPOLICE_DRIVER_CAR3_ defined
