    0407: store_coords_to 888@ 889@ 890@    from_car TANKY_2 with_offset 0.0 40.0 1.0 
    0948: create_explosion_at  888@ 889@ 890@   type 7 camera_shake 2.0

    TANKMAN2 = actor.CreateAsDriver(TEAM_A, RANDOM_ENEMY_MODEL,TANKY_2) 
  
    actor.Health(TANKMAN2) =  HEALTH_LEVEL
    actor.WeaponAccuracy(TANKMAN2) = ACCURACY_LEVEL
    actor.GiveWeaponAndAmmo(TANKMAN2, WEAPONS_STYLE, 99999)
    0446: set_actor TANKMAN2 dismemberment_possible HEADSHOT_MODE
    07FE: set_actor TANKMAN2 fighting_style_to FIGHTING_STYLE moves 6
    07DD: set_actor TANKMAN2 attack_rate FIRE_RATE // previously known as temper_to
    0946: set_actor TANKMAN2 actions_uninterupted_by_weapon_fire FLINCH 
    03FE: set_actor TANKMAN2 cash_give MONEY_LEVEL
    04D8: set_actor TANKMAN2 drowns_in_water WATER_DROWNS
    09B5: set_actor TANKMAN2 signal_after_kill 0 
    060B: set_actor TANKMAN2 decision_maker_to 780@      
    077A: set_actor TANKMAN2 acquaintance PLAYER_HATE to_actors_pedtype 0 // see ped.dat
    077A: set_actor TANKMAN2 acquaintance PLAYER_HATE to_actors_pedtype 1 // see ped.dat
                                              
                          
    yMARKER33y = marker.CreateAboveCar(TANKY_2)
    marker.SetColor(yMARKER33y,0x992424FF)
    marker.SetIconSize(yMARKER33y, 1)
    018B: set_marker yMARKER33y radar_mode MARKER_RADAR_MODE
                                                            
    
          
         if  and
        $notoriety >= 1 
        $notoriety < 10
        then
         0397: enable_car TANKY_2 siren 1
       00AE: set_car TANKY_2 traffic_behaviour_to 2
        00AD: hostile_car TANKY_2 max_speed_to 120.0
        car.SetDriverBehaviour(TANKY_2 , DriveToPlayer)  
        end 
         if
        $notoriety >= 10
        then
                 0397: enable_car TANKY_2 siren 1
        00AE: set_car TANKY_2 traffic_behaviour_to 2
        00AD: hostile_v2_car TANKY_2 max_speed_to 190.0
        car.SetDriverBehaviour(TANKY_2, KillThePlayer) 
        end 
    end   
    
    
    
    if  and
    4_GROUND_TIMER > 5000
    $notoriety >= 8
    856D: not  actor TANKMAN3 defined
    856E: not  car TANKY_3 defined

    then    
    4_GROUND_TIMER = 0   
    TANKY_3 = car.Create(#RHINO, 883@, 884@, 885@)
    car.Health(TANKY_3) = 6000
    car.SetToPsychoDriver(TANKY_3) 
    0840: link_car TANKY_3 to_interior 983@ 
    
          03AB: set_car TANKY_3 strong 1
    gosub @WAVEs
    0407: store_coords_to 888@ 889@ 890@    from_car TANKY_3 with_offset 0.0 30.0 1.0 
    0948: create_explosion_at  888@ 889@ 890@   type 7 camera_shake 2.0

    TANKMAN3 = actor.CreateAsDriver(TEAM_A, RANDOM_ENEMY_MODEL,TANKY_2) 
  
    actor.Health(TANKMAN3) =  HEALTH_LEVEL
    actor.WeaponAccuracy(TANKMAN3) = ACCURACY_LEVEL
    actor.GiveWeaponAndAmmo(TANKMAN3, WEAPONS_STYLE, 99999)
    0446: set_actor TANKMAN3 dismemberment_possible HEADSHOT_MODE
    07FE: set_actor TANKMAN3 fighting_style_to FIGHTING_STYLE moves 6
    07DD: set_actor TANKMAN3 attack_rate FIRE_RATE // previously known as temper_to
    0946: set_actor TANKMAN3 actions_uninterupted_by_weapon_fire FLINCH 
    03FE: set_actor TANKMAN3 cash_give MONEY_LEVEL
    04D8: set_actor TANKMAN3 drowns_in_water WATER_DROWNS
    09B5: set_actor TANKMAN3 signal_after_kill 0 
    060B: set_actor TANKMAN3 decision_maker_to 780@      
    077A: set_actor TANKMAN3 acquaintance PLAYER_HATE to_actors_pedtype 0 // see ped.dat
    077A: set_actor TANKMAN3 acquaintance PLAYER_HATE to_actors_pedtype 1 // see ped.dat
                                              
                          
    yMARKER34y = marker.CreateAboveCar(TANKY_3)
    marker.SetColor(yMARKER34y,0x992424FF)
    marker.SetIconSize(yMARKER34y, 1)
    018B: set_marker yMARKER34y radar_mode MARKER_RADAR_MODE
                                                            
    
          
         if  and
        $notoriety >= 1 
        $notoriety < 10
        then
         0397: enable_car TANKY_3 siren 1
       00AE: set_car TANKY_3 traffic_behaviour_to 2
        00AD: hostile_car TANKY_3 max_speed_to 120.0
        car.SetDriverBehaviour(TANKY_3 , DriveToPlayer)  
        end 
         if
        $notoriety >= 10
        then
                 0397: enable_car TANKY_3 siren 1
        00AE: set_car TANKY_3 traffic_behaviour_to 2
        00AD: hostile_v2_car TANKY_3 max_speed_to 190.0
        car.SetDriverBehaviour(TANKY_3, KillThePlayer) 
        end 
    end 
    
    
    if  and
    4_GROUND_TIMER > 5000
    $notoriety >= 9
    856D: not  actor TANKMAN4 defined
    856E: not  car TANKY_4 defined

    then    
    4_GROUND_TIMER = 0   
    TANKY_4 = car.Create(#RHINO, 883@, 884@, 885@)
    car.Health(TANKY_4) = 6000
    car.SetToPsychoDriver(TANKY_4) 
    0840: link_car TANKY_4 to_interior 983@ 
    
          03AB: set_car TANKY_4 strong 1
    gosub @WAVEs
    0407: store_coords_to 888@ 889@ 890@    from_car TANKY_4 with_offset 0.0 30.0 1.0 
    0948: create_explosion_at  888@ 889@ 890@   type 7 camera_shake 2.0

    TANKMAN4 = actor.CreateAsDriver(TEAM_A, RANDOM_ENEMY_MODEL,TANKY_2) 
  
    actor.Health(TANKMAN4) =  HEALTH_LEVEL
    actor.WeaponAccuracy(TANKMAN4) = ACCURACY_LEVEL
    actor.GiveWeaponAndAmmo(TANKMAN4, WEAPONS_STYLE, 99999)
    0446: set_actor TANKMAN4 dismemberment_possible HEADSHOT_MODE
    07FE: set_actor TANKMAN4 fighting_style_to FIGHTING_STYLE moves 6
    07DD: set_actor TANKMAN4 attack_rate FIRE_RATE // previously known as temper_to
    0946: set_actor TANKMAN4 actions_uninterupted_by_weapon_fire FLINCH 
    03FE: set_actor TANKMAN4 cash_give MONEY_LEVEL
    04D8: set_actor TANKMAN4 drowns_in_water WATER_DROWNS
    09B5: set_actor TANKMAN4 signal_after_kill 0 
    060B: set_actor TANKMAN4 decision_maker_to 780@      
    077A: set_actor TANKMAN4 acquaintance PLAYER_HATE to_actors_pedtype 0 // see ped.dat
    077A: set_actor TANKMAN4 acquaintance PLAYER_HATE to_actors_pedtype 1 // see ped.dat
                                              
                          
    yMARKER35y = marker.CreateAboveCar(TANKY_4)
    marker.SetColor(yMARKER35y,0x992424FF)
    marker.SetIconSize(yMARKER35y, 1)
    018B: set_marker yMARKER35y radar_mode MARKER_RADAR_MODE
                                                            
    
          
         if  and
        $notoriety >= 1 
        $notoriety < 10
        then
         0397: enable_car TANKY_4 siren 1
       00AE: set_car TANKY_4 traffic_behaviour_to 2
        00AD: hostile_car TANKY_4 max_speed_to 120.0
        car.SetDriverBehaviour(TANKY_4 , DriveToPlayer)  
        end 
         if
        $notoriety >= 10
        then
                 0397: enable_car TANKY_4 siren 1
        00AE: set_car TANKY_4 traffic_behaviour_to 2
        00AD: hostile_v2_car TANKY_4 max_speed_to 190.0
        car.SetDriverBehaviour(TANKY_4, KillThePlayer) 
        end 
    end       
end 
///////////despawn////////////////////////////////


if  and
  056D:    actor TANKMAN1 defined
  056E:    car TANKY_1 defined
then
    if or
    $NOTOriety == 0
    car.Wrecked(TANKY_1)
    8205:  not  actor $PLAYER_ACTOR near_car TANKY_1 radius 185.0 185.0 185.0 flag 0
    then   
        marker.Disable(yMARKER32y)
        car.RemoveReferences(TANKY_1)
        actor.RemoveReferences(TANKMAN1)
      
    end
end  
if  and
  056D:    actor TANKMAN2 defined
  056E:    car TANKY_2 defined
then
    if or
    $NOTOriety == 0
    car.Wrecked(TANKY_2)
    8205:  not  actor $PLAYER_ACTOR near_car TANKY_2 radius 185.0 185.0 185.0 flag 0
    then   
            marker.Disable(yMARKER33y)
        car.RemoveReferences(TANKY_2)
        actor.RemoveReferences(TANKMAN2)
      
    end
end  
if  and
  056D:    actor TANKMAN3 defined
  056E:    car TANKY_3 defined
then
    if or
    $NOTOriety == 0
    car.Wrecked(TANKY_3)
    8205:  not  actor $PLAYER_ACTOR near_car TANKY_3 radius 185.0 185.0 185.0 flag 0
    then 
        marker.Disable(yMARKER34y)  
        car.RemoveReferences(TANKY_3)
        actor.RemoveReferences(TANKMAN3)
      
    end
end  
if  and
  056D:    actor TANKMAN4 defined
  056E:    car TANKY_4 defined
then
    if or
    $NOTOriety == 0
    car.Wrecked(TANKY_4)
    8205:  not  actor $PLAYER_ACTOR near_car TANKY_4 radius 185.0 185.0 185.0 flag 0
    then   
        marker.Disable(yMARKER35y)
        car.RemoveReferences(TANKY_4)
        actor.RemoveReferences(TANKMAN4)
      
    end
end  

return 

:RECORDS

if 
002D: STAT_LONGEST_WANTED_LEVEL12 >=  get_longest_wantedlevel12
then
    0085: get_longest_wantedlevel12 = STAT_LONGEST_WANTED_LEVEL12  
end
if 
002D: STAT_LONGEST_WANTED_LEVEL >=  get_longest_wantedlevel
then
    0085: get_longest_wantedlevel = STAT_LONGEST_WANTED_LEVEL  
end
STAT_LONGEST_WANTED_LEVEL = 0
STAT_LONGEST_WANTED_LEVEL12 = 0
return 
if 
002D: STAT_LONGEST_WANTED_LEVEL >=  get_longest_wantedlevel
then
    0085: get_longest_wantedlevel = STAT_LONGEST_WANTED_LEVEL  
end
STAT_LONGEST_WANTED_LEVEL = 0
STAT_LONGEST_WANTED_LEVEL12 = 0
return  
